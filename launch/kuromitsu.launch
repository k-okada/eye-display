<launch>
    <arg name="port" />
    <arg name="baud" default="57600" />
    <arg name="mode_right" default="true" />
    <arg name="direction" default="1" />

    <node
        pkg="rosserial_python"
        type="serial_node.py"
        name="eye_display"
        output="screen"
        >
        <rosparam subst_value="true">
            port: $(arg port)
            baud: $(arg baud)
            mode_right: $(arg mode_right)
            direction: $(arg direction)
            eye_asset_names: [normal, blink, sleepy, sad, troubled, delighed, heart, shine1, shine2, boring, regret1, regret2]
            normal_eye_asset: '{path_iris: "", path_pupil:"", path_reflex: "", path_upperlid: "", upperlid_position: [9]}'
            blink_eye_asset: '{path_iris: "", path_pupil:"", path_reflex: "", path_upperlid: "", upperlid_position: [9, 9, 130, 130, 9, 9]}'
            sleepy_eye_asset: '{path_iris: "", path_pupil:"", path_reflex: "", path_upperlid: "", upperlid_position: [60, 60, 70, 130, 130, 130, 70, 60, 60, 60]}'
            sad_eye_asset: '{path_iris: "", path_pupil:"", path_reflex: "", path_upperlid: "", upperlid_position: [130]}'
            troubled_eye_asset: '{path_iris: "", path_pupil:"", path_reflex: "", path_upperlid: "", upperlid_position: [130]}'
            delighted_eye_asset: '{path_iris: "", path_pupil:"", path_reflex: "", path_upperlid: "", upperlid_position: [130]}'
            heart_eye_asset: '{path_iris: "", path_pupil:"", path_reflex: "", path_upperlid: "", upperlid_position: [130]}'
            shine1_eye_asset: '{path_iris: "", path_pupil:"", path_reflex: "", path_upperlid: "", upperlid_position: [130]}'
            shine2_eye_asset: '{path_iris: "", path_pupil:"", path_reflex: "", path_upperlid: "", upperlid_position: [130]}'
            boring_eye_asset: '{path_iris: "", path_pupil:"", path_reflex: "", path_upperlid: "", upperlid_position: [130]}'
            regret1_eye_asset: '{path_iris: "", path_pupil:"", path_reflex: "", path_upperlid: "", upperlid_position: [130]}'
            regret2_eye_asset: '{path_iris: "", path_pupil:"", path_reflex: "", path_upperlid: "", upperlid_position: [130]}'
        </rosparam>
    </node>
</launch>
